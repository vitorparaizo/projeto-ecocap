{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    { "src": "/", "dest": "/public/telaLogin.html" },
    { "src": "/home", "dest": "/public/home.html" },
    { "src": "/mapa", "dest": "/public/mapa.html" },
    { "src": "/coleta", "dest": "/public/coleta.html" },
    { "src": "/catadores", "dest": "/public/catadores.html" },
    { "src": "/agenda", "dest": "/public/agenda.html" },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ]
}
